#include "juk3.h"

int		main(int nb_arg, char **str)
{
	if (nb_arg > 1)
	{
		int i;
		int j;
		
		i = 0;
		ft_putstr("we have input\n");
		while (str[i])
			i++;
		i--;
		j = 0;
		str[j] = '\0'; // to fix the segfau i put a str term at the front
		while (str[i])
		{
			ft_putstr(str[i]);
			ft_putchar(' ');
			i--;
//			if (str[i] == 0)
//				break;
		}
		ft_putstr("\n fin \n");
		return (0);
	}
}
